#include <iostream>
using namespace std;
int LadderDP(int n,int *dp,int k)
{
    if(n==0)
      return 1;
    if(n<=0)
      return 0;
    if(dp[n]!=0)
      return dp[n];
    int ans=0;
    for(int i=1;i<=k;i++)
       ans=ans+LadderDP(n-i,dp,k);
    return ans;
    //return LadderDP(n-1,dp) + LadderDP(n-2,dp) + LadderDP(n-3,dp);
}
int main()
{
int n,k; 
cin>>n>>k;
int *dp =new int[n];
for(int i=0;i<n;i++)
       dp[i]=0;
cout<<LadderDP(n,dp,k);
return 0;
}
